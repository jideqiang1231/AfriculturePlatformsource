<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".specialist.activity.zhongzi.SpecialistseedpushActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageButton
            android:id="@+id/ib_Specialistseedpush_back"
            android:layout_width="44dp"
            android:layout_height="35dp"
            android:background="@drawable/top_bar_left_back"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="10dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:paddingRight="50dp"
            android:text="我推送的种子"
            android:textColor="#464646"
            android:textSize="20sp" />

        <ImageButton
            android:id="@+id/ib_pecialist_seed_addpush"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@drawable/top_bar_right_addpush"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="10dp"/>


    </LinearLayout>
    <ListView
        android:id="@+id/lv_specialist_zhongzipush"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>